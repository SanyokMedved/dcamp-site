<?php

/**
 * @file
 * Contains dc_core.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function dc_core_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the dc_core module.
    case 'help.page.dc_core':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Drupal Camp Core Features') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function dc_core_theme() {
  return [
    'dc_core' => [
      'render element' => 'children',
    ],
  ];
}
